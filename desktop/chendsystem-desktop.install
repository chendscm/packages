post_install() {
    systemctl enable libvirtd org.cups.cupsd
    sed -i 's/basic/desktop/g' /etc/docker-btrfs.json
    cp /opt/chendsystem/etc/libvirt/qemu.conf /etc/libvirt/qemu.conf
    ln -s /opt/cuda /usr/local/cuda

    # eaf dependencies
    cd /opt/chendsystem/app
    git clone --depth=1 -b master https://github.com/emacs-eaf/emacs-application-framework.git
    cd emacs-application-framework
    chmod +x ./install-eaf.py
    ./install-eaf.py

    sh /opt/chendsystem/sh/install-eaf.sh
}

